---
export const prerender = true;

import Layout from "../../layouts/Layout.astro";

let title = "Projects";
---

<Layout title={title}>
  <div class="m-auto max-w-screen-md p-2">
    <div class="card-compact card bg-white shadow-xl">
      <div class="card-body m-2 text-mainred">
        <h2 class="card-title text-3xl">
          Weekly median apartment prices in vienna
        </h2>
        <p>
          Crawled directly from apartment listings, twice a week. No filter,
          just the median.
        </p>
        <p class="text-xl">
          <strong>
            Have a look at <a
              class="text-link"
              href="/projects/weekly-median-apartment-price"
              >the project breakdown</a
            >.</strong
          >
        </p>
      </div>
    </div>
  </div>
</Layout>
